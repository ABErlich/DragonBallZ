	@Test
	public void testColocarYMoverAGoku() {
		Tablero tablero = new Tablero(10);
		Goku goku = new Goku();
		goku.Ubicar(new Coordenada(1,1));
		goku.mover(new Coordenada(3,3));
		
		tablero.obtenerPosicionDe(goku);
		
		
	}

