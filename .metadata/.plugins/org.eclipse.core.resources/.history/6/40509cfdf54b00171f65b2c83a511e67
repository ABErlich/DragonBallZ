package fiuba.algo3.modelo.tablero;

public class Coordenada {
	
	public Coordenada(int pCoordX, int pCoordY){
		this.CoordX = pCoordX;
		this.CoordY = pCoordY;
	}
	
	private int CoordX;
	private int CoordY;
	
	public int getCoordX() {
		return CoordX;
	}
	public int getCoordY() {
		return CoordY;
	}
	
}
