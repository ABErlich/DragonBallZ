
public class Coordenada {
	
	public Coordenada(int pCoordX, int pCoordY){
		this.CoordX = pCoordX;
		this.CoordY = pCoordY;
	}
	
	private int CoordX;
	private int CoordY;
	
	public int getCoordX() {
		return CoordX;
	}
	public void setCoordX(int coordX) {
		CoordX = coordX;
	}
	public int getCoordY() {
		return CoordY;
	}
	public void setCoordY(int coordY) {
		CoordY = coordY;
	}
	
}
